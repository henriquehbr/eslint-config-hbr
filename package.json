{
  "name": "eslint-config-hbr",
  "version": "1.0.0-beta.3",
  "main": "index.js",
  "author": "Henrique Borges <henriqueborgeshbr@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "changelog": "auto-changelog -l false --hide-credit",
    "postchangelog": "git add CHANGELOG.md && cross-env HUSKY_SKIP_HOOKS=1 git commit --amend --no-edit --no-verify",
    "publish": "yarn changelog"
  },
  "husky": {
    "hooks": {
      "post-commit": "yarn changelog -u"
    }
  },
  "devDependencies": {
    "@types/eslint": "^7.2.4",
    "@types/node": "^14.14.6",
    "auto-changelog": "^2.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "husky": "^4.3.0"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-plugin-markdown": "^2.0.0-alpha.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "typescript": "^4.0.5"
  }
}
