{
  "name": "eslint-config-hbr",
  "version": "1.0.0-beta.2",
  "main": "index.js",
  "author": "Henrique Borges <henriqueborgeshbr@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "changelog": "auto-changelog -l false --hide-credit",
    "postchangelog": "git add CHANGELOG.md && cross-env HUSKY_SKIP_HOOKS=1 git commit --amend --no-edit --no-verify",
    "publish": "yarn changelog"
  },
  "husky": {
    "hooks": {
      "post-commit": "yarn changelog -u"
    }
  },
  "devDependencies": {
    "@types/eslint": "^7.2.2",
    "@types/node": "^14.6.2",
    "auto-changelog": "^2.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.0",
    "husky": "^4.2.5"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.8.0",
    "eslint-plugin-markdown": "^2.0.0-alpha.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "typescript": "^4.0.3"
  }
}
